{
  "name": "roman-numeral-set",
  "version": "1.0.0-0",
  "description": "Convertion from indo-arabic to roman.",
  "repository": {
    "url": "https://github.com/rplaurindo/roman-numeral-set"
  },
  "license": "MPL-2.0",
  "devDependencies": {
    "@types/node": "~14.14.37",
    "typescript": "~4.0.2",
    "@actjs.on/cop-fy": "1.0.0-18"
  },
  "scripts": {
    "build:prepare": "git pull && tsc -b",
    "rwin:set-cop-fy-settings-directory": "npm set ACTJS_ON_SETTINGS_DIRECTORY=%INIT_CWD%/settings",
    "rwin:cop-fy:copy-files": "npm run rwin:set-cop-fy-settings-directory && npm explore @actjs.on/cop-fy -- npm run copy-files",
    "rwin:build": "npm run build:prepare && npm run rwin:cop-fy:copy-files",
    "test:human": "node dist/@actjs.on/roman-numeral-set/tests/human"
  }
}
